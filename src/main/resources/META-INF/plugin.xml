<idea-plugin>
    <id>org.jetbrains</id>
    <name>Navigation wheel</name>
    <version>0.0.1</version>
    <vendor email="skyd96@gmail.com" url="skydi17">skydi17</vendor>

    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
    </extensions>

    <actions>
        <action id="Myplugin.Textboxes" class="plugin.actions.WheelPlugin" text="Open" description="Open wheel plugin">
            <keyboard-shortcut first-keystroke="control alt G" second-keystroke="C" keymap="$default"/>
        </action>
        <action id="closeFile" class="plugin.actions.CloseFile" text="Close" description="Close file in wheel plugin">

        </action>
        <action id="pinFile" class="plugin.actions.PinFile" text="Pin" description="Pin file in wheel plugin">

        </action>
        <action id="openFile" class="plugin.actions.OpenFile" text="Open" description="Open file in IDE">

        </action>
    </actions>

</idea-plugin>